{
    "name": "trungdv/zalo-otp",
    "description": "Zalo OTP Package for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "TrungDV",
        "email": "trungdv@example.com"
    }],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0|^11.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "TrungDV\\ZaloOtp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TrungDV\\ZaloOtp\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TrungDV\\ZaloOtp\\ZaloOtpServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}